    this.newProduct = {
      availabilityStatus: "",
      brand: "",
      category: "",
      price: 0,
      productId: 0,
      rating: 0,
      sku: "",
      stock: 0,
      title: "",
      images: []
    };
    this.imagePreviews = [];
  }
