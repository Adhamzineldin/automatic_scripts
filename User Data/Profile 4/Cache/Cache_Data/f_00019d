var CategoryComponent = class _CategoryComponent {
  productService;
  sanitizer;
  authService;
  router;
  categoryTitle = "";
  products = [];
  imageUrl;
  isAdmin = false;
  constructor(productService, sanitizer, authService, router) {
    this.productService = productService;
    this.sanitizer = sanitizer;
    this.authService = authService;
    this.router = router;
  }
  ngOnInit() {
    this.isAdmin = this.authService.isAdmin();
    console.log("isAdmin:", this.isAdmin);
  }
  getStars(rating) {
    const filledStars = "\u2605".repeat(Math.floor(rating));
    const emptyStars = "\u2606".repeat(5 - Math.floor(rating));
    return filledStars + emptyStars;
  }
  getLink(filename) {
    let fileUrl = "";
    if (typeof filename === "string" && filename.length > 0) {
      if (filename.startsWith("/")) {
        fileUrl = `${AppComponent.api}/images${filename}`;
      } else {
        fileUrl = filename;
      }
    } else {
      console.warn("Invalid filename provided:", filename);
    }
    return fileUrl;
  }
  confirmDelete(productId) {
    const confirmation = window.confirm("Are you sure you want to delete this product? This action cannot be undone.");
    if (confirmation) {
      this.deleteProduct(productId);
    }
  }
  deleteProduct(productId) {
    this.authService.deleteProduct(productId).subscribe({
      next: (response) => {
        window.location.reload();
      },
      error: (err) => {
        console.error("Failed to delete product:", err);
        alert("Error deleting product. Please try again later.");
      }
    });
  }
