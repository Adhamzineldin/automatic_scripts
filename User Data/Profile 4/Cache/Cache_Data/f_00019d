var AuthService = class _AuthService {
  http;
  user;
  nodemailer;
  tokenSubject = new BehaviorSubject(null);
  token$ = this.tokenSubject.asObservable();
  constructor(http) {
    this.http = http;
    this.user = JSON.parse(localStorage.getItem("user") || "null");
    console.log("AuthService initialized. Current user:", this.user);
  }
  getCurrentUser() {
    return this.user && this.user.isVerified ? this.user : null;
  }
  getUserEmail() {
    return this.user?.email || "";
  }
  // Method to set user when logged in
  setUser(userData) {
    this.user = userData;
    localStorage.setItem("user", JSON.stringify(userData));
  }
  register(userData) {
    userData.username = userData.username.toLowerCase();
    return this.http.post(`${AppComponent.api}/users/register`, userData);
  }
  verifyEmail(email) {
    return this.http.post(`${AppComponent.api}/users/verify-email`, { email }).pipe(map((response) => {
      if (response.message === "success") {
        return { success: true, message: "Email verification successful" };
      }
      return { success: false, message: "Email verification failed" };
    }));
  }
  sendCodeToEmail(email, code) {
    const payload = { email, code };
    return this.http.post(`${AppComponent.api}/users/send-verification-code`, payload).pipe(catchError((error) => {
      console.error("Error sending verification code:", error);
      return throwError(error);
    }));
  }
  login(email, password) {
    email = email.toLowerCase();
    return this.http.post(`${AppComponent.api}/users/login`, { email, password }).pipe(map((response) => {
      this.setUser(response.user);
      this.storeToken(response.token);
      return response;
    }), catchError(this.handleError));
  }
  // Logout a user
  logout() {
    this.tokenSubject.next(null);
    localStorage.removeItem("authToken");
  }
  // Check if the user is logged in
  isLoggedIn() {
    return !!this.getToken();
  }
  // Get the token
  getToken() {
    return localStorage.getItem("authToken");
  }
  // Store the token
  storeToken(token) {
    localStorage.setItem("authToken", token);
    this.tokenSubject.next(token);
  }
  // Handle errors from HTTP requests
  handleError(error) {
    console.error("An error occurred:", error);
    throw error;
  }
  addProductToCart(productId) {
    console.log("productId", productId);
    let userId = this.getCurrentUser()?.userId;
    const body = { userId, productId };
    return this.http.post(`${AppComponent.api}/users/add-to-cart`, body);
  }
  getCart() {
    return this.http.get(`${AppComponent.api}/users/cart/${this.getCurrentUser()?.userId}`);
  }
  getProductDetails(productId) {
    return this.http.get(`${AppComponent.api}/products/product/${productId}`);
  }
  updateUserCart(updatedCart) {
    const userId = this.getCurrentUser()?.userId;
    return this.http.put(`${AppComponent.api}/users/cart/${userId}`, { updatedCart });
  }
  // Change password
  changePassword(oldPassword, newPassword) {
    const email = this.getCurrentUser()?.email;
    return this.http.put(`${AppComponent.api}/users/update-password`, { email, oldPassword, newPassword });
  }
  forgotPassword(email, newPassword) {
    return this.http.post(`${AppComponent.api}/users/forgot-password`, { email, newPassword });
  }
  updateOrder(userId, updatedOrder) {
    console.log("updatedOrder", updatedOrder);
    return this.http.put(`${AppComponent.api}/users/orders/${userId}`, { "updatedOrder": updatedOrder });
  }
  getOrders(userId) {
    return this.http.get(`${AppComponent.api}/users/orders/${userId}`);
  }
  getOrderDetails(orderNumber) {
    const userId = this.getCurrentUser()?.userId;
    return this.http.get(`${AppComponent.api}/users/orders/order/${userId}?orderNumber=${orderNumber}`);
  }
  //   admin
  getUsers() {
    return this.http.get(AppComponent.api);
  }
  deactivateUser(userId) {
    return this.http.delete(`${AppComponent.api}/${userId}/deactivate`);
  }
  addProduct(product) {
    console.log("product", product);
    return this.http.post(`${AppComponent.api}/products`, product);
  }
  deleteProduct(productId) {
    return this.http.delete(`${AppComponent.api}/products/product/${productId}`);
  }
  getProducts() {
    return this.http.get(`${AppComponent.api}/products`);
  }
  isAdmin() {
    return this.getCurrentUser()?.roles.includes("admin");
  }
  sendEmail(userId, emailContent) {
    const email = this.getCurrentUser()?.email;
    console.log("sending email");
    return this.http.post(`${AppComponent.api}/users/sendEmail`, { htmlContent: emailContent, email });
  }
  updateProductStock(productId, newStock) {
    return this.http.put(`${AppComponent.api}/products/${productId}/stock`, { stock: newStock });
  }
